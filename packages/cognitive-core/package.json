{
  "name": "redux-brain-medical-ai",
  "version": "1.0.1",
  "description": "Medical AI decision engine for LATAM healthcare - Intelligent triage, SOAP generation, and clinical decision support",
  "author": "Bernard Orozco",
  "license": "ISC",
  "keywords": [
    "medical",
    "ai",
    "healthcare",
    "triage",
    "soap",
    "clinical",
    "redux",
    "claude",
    "anthropic",
    "latam",
    "telemedicine"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BernardUriza/redux-claude"
  },
  "bugs": {
    "url": "https://github.com/BernardUriza/redux-claude/issues"
  },
  "homepage": "https://github.com/BernardUriza/redux-claude#readme",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./src/store/selectors": {
      "import": "./dist/store/selectors/index.js",
      "types": "./dist/store/selectors/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.bypass.json",
    "build:strict": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@reduxjs/toolkit": "^2.8.2",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}
